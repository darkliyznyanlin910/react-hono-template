{
  "name": "react-hono-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "packageManager": "bun@1.2.15",
  "engines": {
    "node": ">=22.14.0",
    "bun": ">=1.2.15"
  },
  "imports": {
    "#server/*": "./server/*",
    "#client/*": "./src/*"
  },
  "scripts": {
    "start": "NODE_ENV=production bun with-env node dist/index.cjs",
    "build": "bun with-env vite build && esbuild server/index.ts --bundle --platform=node --outfile=dist/index.cjs",
    "preview": "bun with-env vite preview",
    "clean": "git clean -xdf .cache .turbo dist node_modules .tanstack",
    "dev:server": "bun with-env tsx watch server/index.ts",
    "dev:vite": "wait-on http://localhost:4001/health && bun with-env vite",
    "dev": "concurrently \"bun dev:server\" \"bun dev:vite\"",
    "format": "prettier --check . --ignore-path .gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e .env --",
    "test": "vitest run",
    "auth:generate": "bun dlx @better-auth/cli generate --output server/drizzle/schema/auth.ts --config server/lib/auth.ts",
    "db:push": "bun with-env drizzle-kit push",
    "db:studio": "bun with-env drizzle-kit studio"
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "@hono/node-server": "^1.14.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/react-table": "^8.21.2",
    "better-auth": "^1.3.7",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.1",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.7",
    "@eslint/compat": "^1.3.2",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/router-devtools": "^1.99.9",
    "@tanstack/router-plugin": "^1.99.12",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "3.2.4",
    "concurrently": "^9.2.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-turbo": "^2.5.6",
    "globals": "^15.14.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "6.1.0",
    "wait-on": "^8.0.4"
  },
  "prettier": "./prettier.config.js"
}
