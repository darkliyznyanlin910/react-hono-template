{
  "name": "react-hono-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.11.0"
  },
  "packageManager": "pnpm@10.11.1",
  "scripts": {
    "start": "NODE_ENV=production pnpm with-env node dist/index.cjs",
    "build": "pnpm with-env vite build && esbuild server/index.ts --bundle --platform=node --outfile=dist/index.cjs",
    "preview": "pnpm with-env vite preview",
    "clean": "git clean -xdf .cache .turbo dist node_modules .tanstack",
    "dev:server": "pnpm with-env tsx watch server/index.ts",
    "dev:vite": "wait-on http://localhost:4001/health && pnpm with-env vite",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:vite\"",
    "format": "prettier --check . --ignore-path .gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e .env --",
    "test": "vitest run"
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "@hono/node-server": "^1.14.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/react-table": "^8.21.2",
    "hono": "^4.9.1",
    "next-themes": "^0.4.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/router-devtools": "^1.99.9",
    "@tanstack/router-plugin": "^1.99.12",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "3.2.4",
    "concurrently": "^9.2.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-turbo": "^2.5.6",
    "globals": "^15.14.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "6.1.0",
    "wait-on": "^8.0.4"
  },
  "prettier": "./prettier.config.js"
}
